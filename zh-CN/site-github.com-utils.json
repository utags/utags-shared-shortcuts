{
  "id": "github_utils",
  "name": "GitHub Utils",
  "icon": "url:https://github.com/favicon.ico",
  "match": [
    "!/https://github\\.com/(topics|collections|trending|resources)/.*/",
    "/https://github\\.com/[\\w-]+/[\\w-]+(/.*)?$/"
  ],
  "defaultOpen": "new-tab",
  "items": [
    {
      "id": "githubdev",
      "name": "GitHub.dev",
      "type": "url",
      "data": "https://github.dev/{p:1||t:utags}/{p:2||t:utags}",
      "openIn": "new-tab",
      "icon": "lucide:code-xml"
    },
    {
